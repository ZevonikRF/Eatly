{
  "expo": {
    "name": "Eatly",
    "slug": "Eatly",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-eatly.png",
    "scheme": "Eatly",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-eatly-new.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-eatly.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jennifergrachel.Eatly"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "72eba694-5050-4b57-88d0-684518ee556a"
      }
    }
  }
}
